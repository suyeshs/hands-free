{
  "version": "1.1.0",
  "lastUpdated": "2025-12-22",
  "platforms": {
    "swiggy": {
      "enabled": true,
      "dashboardUrl": "https://partner.swiggy.com/",
      "selectors": {
        "orderContainer": "[data-testid='last_4_digits_order_number']",
        "orderId": "[data-testid='last_4_digits_order_number']",
        "orderNumber": "[data-testid='last_4_digits_order_number']",
        "orderDetailsText": "[data-testid='ordertime_amount_items_text']",
        "customerName": "[data-testid*='customer'], [class*='customer-name']",
        "customerPhone": "[data-testid*='phone'], [class*='customer-phone']",
        "customerAddress": "[data-testid*='address'], [class*='delivery-address']",
        "itemsList": "[data-testid='ordertime_amount_items_text']",
        "itemRow": "[data-testid*='item'], [class*='item-row']",
        "itemName": "[data-testid*='item-name'], [class*='dish-name']",
        "itemQuantity": "[data-testid*='quantity'], [class*='qty']",
        "itemPrice": "[data-testid*='price'], [class*='price']",
        "itemModifiers": "[data-testid*='customization'], [class*='variant']",
        "specialInstructions": "[data-testid*='cutlery'], [data-testid*='instructions'], [class*='notes']",
        "orderTotal": "[data-testid='ordertime_amount_items_text']",
        "orderStatus": ".sc-gtLWhw .sc-blHHSb, [class*='status']",
        "platformBadge": ".sc-gtLWhw",
        "orderTime": "[data-testid='ordertime_amount_items_text']",
        "moreButton": "[data-testid='more_button']",
        "acceptButton": "[data-testid*='accept'], button[class*='accept']",
        "rejectButton": "[data-testid*='reject'], button[class*='reject']",
        "readyButton": "[data-testid*='ready'], button[class*='ready']",
        "tabNew": "[data-testid*='new'], [class*='tab-new']",
        "tabPreparing": "[data-testid*='preparing'], [class*='tab-preparing']",
        "tabReady": "[data-testid*='ready'], [class*='tab-ready']",
        "tabPickedUp": "[data-testid*='picked'], [class*='tab-picked']",
        "tabPastOrders": "[data-testid*='past'], [class*='tab-past']"
      },
      "attributes": {
        "orderId": "aria-label",
        "orderNumber": "aria-label",
        "orderStatus": "data-testid",
        "itemQuantity": "data-quantity"
      },
      "polling": {
        "enabled": true,
        "intervalMs": 5000,
        "useObserver": true
      },
      "extraction": {
        "skipProcessedOrders": true,
        "maxOrdersPerScan": 50,
        "parseNumericValues": true
      }
    },
    "zomato": {
      "enabled": true,
      "dashboardUrl": "https://www.zomato.com/partners/login",
      "selectors": {
        "orderContainer": ".order-card, [class*='order-container'], [data-testid*='order']",
        "orderId": ".order-id, [class*='order-id'], [data-order-id]",
        "orderNumber": "[data-order-number], .order-number, #orderId",
        "customerName": ".customer-name, [class*='customer'], [data-customer-name]",
        "customerPhone": ".customer-phone, [class*='phone'], [data-customer-phone]",
        "customerAddress": ".delivery-address, [class*='address']",
        "itemsList": ".items-list, [class*='items'], .order-items",
        "itemRow": ".item, [class*='item-row'], li",
        "itemName": ".item-name, [class*='dish'], [class*='item-name']",
        "itemQuantity": ".quantity, [class*='qty'], [data-quantity]",
        "itemPrice": ".price, [class*='price'], [data-price]",
        "itemModifiers": ".addons, [class*='addon'], [class*='variant']",
        "specialInstructions": ".cooking-instructions, [class*='instructions']",
        "orderTotal": ".order-total, [class*='total-amount']",
        "orderStatus": "[data-status], [class*='status']",
        "orderTime": ".time, [class*='timestamp'], time",
        "acceptButton": "button:contains('Accept'), [class*='accept-btn']",
        "rejectButton": "button:contains('Reject'), [class*='reject-btn']",
        "readyButton": "button:contains('Ready'), [class*='ready-btn']"
      },
      "attributes": {
        "orderId": "data-order-id",
        "orderNumber": "data-order-number",
        "orderStatus": "data-status",
        "itemQuantity": "data-qty"
      },
      "polling": {
        "enabled": true,
        "intervalMs": 5000,
        "useObserver": true
      },
      "extraction": {
        "skipProcessedOrders": true,
        "maxOrdersPerScan": 50,
        "parseNumericValues": true
      }
    }
  },
  "global": {
    "debugMode": false,
    "logExtractions": true,
    "notifyOnNewOrder": true,
    "autoAcceptOrders": false
  },
  "notes": [
    "These are default selectors based on common patterns.",
    "Update selectors after inspecting actual dashboards in browser DevTools.",
    "Multiple selectors are comma-separated and will be tried in order.",
    "Use browser console: document.querySelector('SELECTOR') to test selectors.",
    "To update: Edit this file and restart dashboard windows."
  ]
}

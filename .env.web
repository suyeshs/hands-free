# Web Environment Variables
# Used when building/running the web version (not Tauri)

# Platform identifier
VITE_PLATFORM=web

# Backend API URL
VITE_BACKEND_API_URL=https://stonepot-restaurant-def3r7eewq-uc.a.run.app/api

# Default tenant ID (can be overridden or made dynamic)
VITE_DEFAULT_TENANT_ID=stonepot-main

# Feature flags
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_PRINTING=true
VITE_ENABLE_VOICE_ORDERING=true

# WebSocket URL (for real-time aggregator orders)
VITE_WEBSOCKET_URL=wss://stonepot-restaurant-def3r7eewq-uc.a.run.app

# App metadata
VITE_APP_NAME=Restaurant POS
VITE_APP_VERSION=1.0.0
